From d9352e5e10759043a80540355fe9614400203853 Mon Sep 17 00:00:00 2001
From: garyyiu2015 <garyyiu2015@gmail.com>
Date: Thu, 14 Jun 2018 12:25:36 +0800
Subject: [PATCH] disable auto update via wifi

---
 MiuiSuperMarket/res/xml/preferences.xml | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

--- a/MiuiSuperMarket/res/xml/preferences.xml
+++ b/MiuiSuperMarket/res/xml/preferences.xml
@@ -6,7 +6,7 @@
         <CheckBoxPreference android:title="@string/pref_title_receive_push_message" android:key="pref_key_receive_push_message" android:defaultValue="true" />
     </PreferenceCategory>
     <PreferenceCategory android:title="@string/pref_category_title_other" android:key="category_key_other">
-        <CheckBoxPreference android:title="@string/pref_title_auto_update_via_wifi" android:key="pref_key_auto_update_via_wifi" android:summary="@string/pref_summary_auto_update_via_wifi" android:defaultValue="true" />
+        <CheckBoxPreference android:title="@string/pref_title_auto_update_via_wifi" android:key="pref_key_auto_update_via_wifi" android:summary="@string/pref_summary_auto_update_via_wifi" android:defaultValue="false" />
         <CheckBoxPreference android:title="@string/pref_title_silent_install" android:key="pref_key_silent_install" android:defaultValue="true" />
         <CheckBoxPreference android:title="@string/pref_title_delete_package" android:key="pref_key_delete_package" android:defaultValue="true" />
         <Preference android:title="@string/pref_title_clear_update_record" android:key="pref_key_clear_update_record" />
